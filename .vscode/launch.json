{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/pset4/a.out",
            "args": ["h", "catgrepmore.c"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/pset4/",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb ",
                    "text": "-gdb-set follow-fork-mode parent -enable-pretty-printing ",
                    "ignoreFailures": true,

                }
            ]
        }
    ]
}